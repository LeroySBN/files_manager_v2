{
  "name": "Files by Leroy Web Frontend",
  "version": "0.1.0",
  "description": "Organise your photos and markdown files",
  "main": "index.js",
  "scripts": {
    "build": "webpack build --config ./config/webpack.config.js",
    "start": "webpack-dev-server --mode development --config config/webpack.config.js",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeroySBN/files_manager_v2.git"
  },
  "keywords": [],
  "author": "Leroy Nazoi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LeroySBN/files_manager_v2/issues"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "aphrodite": "^2.4.0",
    "immutable": "^5.0.0-beta.4",
    "inline-source-map": "^0.6.2",
    "jquery": "^3.7.1",
    "node-fetch": "^2.7.0",
    "normalizr": "^3.6.2",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^8.1.2",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/runtime": "^7.23.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^14.5.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "chai": "^5.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "fetch-mock": "^9.11.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "image-webpack-loader": "^8.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mocha": "^10.2.0",
    "react-test-renderer": "^16.14.0",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^node-fetch$": "<rootDir>/__mocks__/node-fetch.js"
    },
    "transform": {
      "\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  }
